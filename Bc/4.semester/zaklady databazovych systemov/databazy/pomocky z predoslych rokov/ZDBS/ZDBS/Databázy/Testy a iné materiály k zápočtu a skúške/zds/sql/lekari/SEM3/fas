ttitle center 'Prijmy za vykony podla obdobia'
set colsep |
set underline *
SET PAGESIZE 60
SET LINESIZE 123
BREAK ON REPORT
COLUMN zoz_ukonov_id_ukonu HEADING "ID ukonu" A8
COLUMN popis_ukonu HEADING "Názov ukonu"
COLUMN spolu HEADING "Príjem | za ukon"
SELECT zoz_ukonov_id_ukonu, popis_ukonu, SUM(cena_ukonu) spolu, dat_vykonu
FROM zoz_ukonov, vyk_ukony
WHERE TO_CHAR(dat_vykonu,'DD.MM.RRRR') >= '&dat_od' AND
TO_CHAR(dat_vykonu,'DD.MM.RRRR') <= '&dat_do' AND
zoz_ukonov_id_ukonu = id_ukonu
GROUP BY zoz_ukonov_id_ukonu,popis_ukonu
ORDER BY zoz_ukonov_id_ukonu
/
